#include <stdio.h>
int main()
{
    int a,b,c;
    
  printf("enter three sides of the triangle:");
  scanf("%d %d %d",&a,&b,&c);
   if(a == b && b==c)
   {
      printf("\nequilateral\n");
   }
   else if(a == b || b == c || c == a)
   {
      printf("Isosceles");
   }
   else
   {
      printf("scalene\n");
   }
    return 0;
}
