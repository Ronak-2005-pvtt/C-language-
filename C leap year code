#include <stdio.h>
int main()
{
    int year;
  printf("enter athe year:");
  scanf("%d",&year);
   if(year % 100 == 0)
   {
      if(year %400 == 0)
      {
      printf("/nLeap Year");
      }
   
   else
      {
      printf("Not a Leap Year");
      }
   }
   else
      if(year % 4 == 0)
      {
      printf("\nLeap Year\n");
      }
    return 0;
}
